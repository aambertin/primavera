{
  "name": "primavera",
  "version": "1.0.12",
  "description": "Decorators-based quick development framework for Node.js",
  "main": "dist/index.js",
  "scripts": {
    "prepack": "rm -Rf dist; mkdir dist; cp config/package.json dist",
    "build": "babel src --presets babel-preset-es2015-node6,babel-preset-stage-3 --out-dir dist",
    "dist": "npm run prepack; npm run build; cd dist; npm pack",
    "prepublish": "npm run build",
    "publish": "cd dist; npm publish",
    "test": "mocha --require babel-register",
    "generate-docs": "node_modules/.bin/jsdoc -c jsdoc.json -d docs src/web.js"
  },
  "author": "Anibal Ambertin <anibal@synergicsource.com> (http://www.synergicsource.com)",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/aambertin/primavera"
  },
  "keywords": [
    "express",
    "expressjs",
    "generator",
    "primavera",
    "spring",
    "framework",
    "rails",
    "django",
    "koa",
    "sails",
    "lambda",
    "aws",
    "microservices",
    "rad",
    "api gateway",
    "cloud"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-decorators": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "docdash": "^0.4.0",
    "jsdoc": "^3.5.5",
    "jsdoc-escape-at": "^1.0.1",
    "mocha": "^3.3.0",
    "should": "^11.2.1"
  },
  "dependencies": {
    "debug": "^3.0.1",
    "find": "^0.2.7",
    "json-stable-stringify": "^1.0.1",
    "jsonschema": "^1.1.1",
    "lodash": "^4.17.4",
    "parent-require": "^1.0.0"
  }
}
